{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build:react": "turbo run build --filter react-app",
    "build:vue-1": "turbo run build --filter vue-app-1",
    "build:vue-2": "turbo run build --filter vue-app-2",
    "check-types": "turbo run check-types",
    "dev:react": "turbo run dev --filter react-app",
    "dev:vue-1": "turbo run dev --filter vue-app-1",
    "dev:vue-2": "turbo run dev --filter vue-app-2",
    "format": "prettier --write \"**/*.{js,ts,tsx,vue,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,vue,json,md}\"",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-vue": "^10.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "turbo": "^2.7.2",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.50.1"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,json,md}": [
      "prettier --write"
    ]
  }
}
